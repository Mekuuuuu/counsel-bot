{
    "version": 2,
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/about",
            "dest": "/about.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
} 